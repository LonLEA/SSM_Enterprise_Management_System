<body class="hold-transition skin-purple sidebar-mini">

<div class="wrapper">

    <!-- 页面头部 -->
    <jsp:include page="../header.jsp"/>
    <!-- 页面头部 /-->

    <!-- 导航侧栏 -->
    <jsp:include page="../aside.jsp"/>
    <!-- 导航侧栏 /-->

    <!-- 内容区域 -->
    <div class="content-wrapper">

        <!-- 内容头部 -->
        <section class="content-header">
            <h1>
                项目管理
                <small>编辑项目</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="${pageContext.request.contextPath}/pages/main.jsp"><i class="fa fa-dashboard"></i> 首页</a></li>
                <li><a href="${pageContext.request.contextPath}/pages/users/user_list.jsp">项目管理</a></li>
                <li class="active">编辑项目</li>
            </ol>
        </section>
        <!-- 内容头部 /-->

        <!-- 正文区域 -->
        <section class="content">
            <form action="${pageContext.request.contextPath}/projects/update.do" method="post">
                <!--订单信息-->
                <div class="panel panel-default">
                    <div class="panel-heading">项目信息</div>
                    <div class="row data-type">

                        <input hidden name="id" value="${projectInfo.id}">

                        <div class="col-md-2 title">项目名</div>
                        <div class="col-md-4 data">
                            <input name="projectName" type="text" class="form-control"
                                   placeholder="${projectInfo.projectName}" value="${projectInfo.projectName}">
                        </div>

                        <div class="col-md-6 data"></div>

                        <div class="col-md-2 title">创建时间</div>
                        <div class="col-md-4 data">
                            <div class="input-group date">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                <input name="beginTime" type="text" class="form-control pull-right datepicker-a3" >
                            </div>
                        </div>
                        <div class="col-md-2 title">结束时间</div>
                        <div class="col-md-4 data">
                            <div class="input-group date">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                <input name="endTime" type="text" class="form-control pull-right datepicker-a3">
                            </div>
                        </div>

                        <div class="col-md-2 title">概述</div>
                        <div class="col-md-10 data">
                            <input name="description" type="text" class="form-control"
                            placeholder="${projectInfo.description}" value="${projectInfo.description}">
                        </div>


                        <div class="col-md-2 title">部门</div>
                        <div class="col-md-4 data">
                            <select name="departmentId" class="form-control select2" style="width: 100%;">
                                <option value="${projectInfo.department.id}">${projectInfo.department.departmentName}</option>
                                <c:forEach var="department1" items="${sessionScope.departments}">
                                    <option value="${department1.id}">${department1.departmentName}</option>
                                </c:forEach>
                            </select>
                        </div>

                        <div class="col-md-2 title">项目状态</div>
                        <div class="col-md-4 data">
                            <select name="status" class="form-control select2" style="width: 100%;">
                                <option value="0" selected="selected">关闭</option>
                                <option value="1">开启</option>
                                <option value="2">完成</option>
                            </select>
                        </div>

                    </div>
                </div>
                <!--订单信息/-->


                <!--工具栏-->
                <div class="box-tools text-center">
                    <button type="submit" class="btn bg-maroon">保存</button>
                    <button type="button" class="btn bg-default" onclick="history.back(-1);">返回</button>
                </div>
                <!--工具栏/-->
            </form>
        </section>
        <!-- 正文区域 /-->

    </div>
    <!-- 内容区域 /-->

    <!-- 底部导航 -->
    <footer class="main-footer">
        <div class="pull-right hidden-xs">
            <b>Version</b> 1.0
        </div>
        <strong>Copyright &copy; 2020 <a href="http://www.lonlea.cn">LonLEA</a>.</strong>
    </footer>
    <!-- 底部导航 /-->

</div>


<script src="${pageContext.request.contextPath}/plugins/jQuery/jquery-2.2.3.min.js"></script>
