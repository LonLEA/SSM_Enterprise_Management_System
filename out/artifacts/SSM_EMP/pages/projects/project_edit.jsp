<script>
    $(document).ready(function() {
        // 选择框
        $(".select2").select2();

        // WYSIHTML5编辑器
        $(".textarea").wysihtml5({
            locale: 'zh-CN'
        });
    });


    // 设置激活菜单
    function setSidebarActive(tagUri) {
        var liObj = $("#" + tagUri);
        if (liObj.length > 0) {
            liObj.parent().parent().addClass("active");
            liObj.addClass("active");
        }
    }


    $(document).ready(function() {
        $('#datepicker-a3').datepicker({
            autoclose: true,
            language: 'zh-CN'
        });
    });


    $(document).ready(function() {
        $('#datepicker-a6').datepicker({
            autoclose: true,
            language: 'zh-CN'
        });
    });


    $(document).ready(function() {
        // 激活导航位置
        setSidebarActive("project-manage");
    });

    $(document).ready(function(){
        $.post({
            url: "${pageContext.request.contextPath}/department/findAll.do",
            dataType: "json", //返回数据类型
            success: function(departments){
            }
        });
    })
    $(document).ready(function() {
        $('.datepicker-a3').datepicker({
            autoclose: true,
            language: 'zh-CN'
        });
    });
</script>
</body>

